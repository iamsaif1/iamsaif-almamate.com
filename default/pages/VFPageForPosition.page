<apex:page standardController="Position__c" recordSetVar="pos" >
  <apex:pageblock title="Position Details">
      <apex:pageBlockSection title="List Of Positions" columns="1">
      <apex:pageBlockTable value="{!pos}" var="p">
          <apex:column value="{!p.Name}"/>
          <apex:column value="{!p.Start_Date__c}"/>
          <apex:column value="{!p.Date_Closed__c}"/>
          <apex:column value="{!p.Salary__c}"/>
          <apex:column ><apex:outputLink value="{!URLFOR($Action.Position__c.Edit,p.id)}">Edit</apex:outputLink>
         </apex:column>
         <apex:column ><apex:outputLink value="{!URLFOR($Action.Position__c.Delete,p.id)}">Delete</apex:outputLink>
         </apex:column>
          <apex:column ><apex:outputLink value="{!URLFOR($Action.Position__c.View,p.id)}">View</apex:outputLink>
         </apex:column>
          </apex:pageBlockTable>
      
      </apex:pageBlockSection>
    </apex:pageblock>
</apex:page>